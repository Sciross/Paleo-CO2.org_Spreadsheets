{
    "version":"0.0.1",
    "root_folder":"./Data/Archive",
    "output_file":"Paleo-CO2_Archive.json",
    "log_file":"log.txt",
    "file_endings":["xls","xlsx"],
    "header_rows":3,
    "missing_value":"NA",
    "use_background_colors":true,
    "background_colors":["grey","black"],
    "use_text_colors":true,
    "text_color_bits":24,
    "text_colors":{"PASS":"#00ff00","WARN":"yellow","FAIL":"red"},
    "properties":
    [
        {
            "name":"proxy",
            "column":"A",
            "type":"text",
            "acceptable_values":["Phytoplankton","Boron isotopes","Stomatal Frequencies","Leaf Gas Exchange","Liverworts","Leaf Carbon Isotopes","Paleosols","Nahcolite/Trona"],
            "match_case":false,
            "required":true
        },
        {
            "name":"age_ka",
            "column":"E",
            "type":"numeric",
            "hard_limits":[0,null]
        },
        {
            "name":"age_uncertainty_pos_ka",
            "column":"F",
            "type":"numeric",
            "hard_limits":[0,null],
            "match_case":false
        },
        {
            "name":"age_uncertainty_neg_ka",
            "column":"G",
            "type":"numeric",
            "hard_limits":[0,null],
            "match_case":false
        },
        {
            "name":"CO2_ppm",
            "column":"H",
            "type":"numeric",
            "hard_limits":[0,null],
            "match_case":false
        },
        {
            "name":"CO2_uncertainty_pos_ppm",
            "column":"I",
            "type":"numeric",
            "hard_limits":[0,null],
            "match_case":false
        },
        {
            "name":"CO2_uncertainty_neg_ppm",
            "column":"J",
            "type":"numeric",
            "hard_limits":[0,null],
            "match_case":false
        },
        {
            "name":"Reference of the data product",
            "column":"M",
            "type":"reference",
            "match_case":false
        },
        {
            "name":"DOI link to reference",
            "column":"N",
            "type":"DOI",
            "match_case":false
        }
    ]
}
